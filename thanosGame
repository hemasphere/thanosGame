                                else if (questionTwo == 1) 
                                {
                                    String[] option5 = {"Odin", "Saturn", "Eternal A'lars"};
                                    int questionThree = JOptionPane.showOptionDialog(null, "Thanos is the son of...", "Question Three", 0, 0, null, option5, null);
                                    if (questionThree != 2) 
                                    {
                                        wrongQuestion();
                                    } 
                                    else 
                                    {
                                        JOptionPane.showMessageDialog(null, "Oh no...", "Success", 0, null);
                                        JOptionPane.showMessageDialog(null, "Mission is successful :)", "End Game", 0, null);
                                    }
                                }
                            }
                        } 
                        else 
                        {
                            JOptionPane.showMessageDialog(null, "Oh, okay well I need to go and sadly must kick you out. Have a safe trip home. Bye", "Bye", 0, null);
                            JOptionPane.showMessageDialog(null, "Mission failed :(", "End Game", 0, null);
                        }
                    }

                } 
                else 
                {
                    JOptionPane.showMessageDialog(null, "I'm Korg, nice to meet you", "Meeting Korg", 0, null);
                    String[] option10 = {"I'm looking for Thanos"};
                    JOptionPane.showOptionDialog(null, "Oh nice. Hey so what are you here for?", "", 0, 0, null, option10, null);
                    int help = JOptionPane.showConfirmDialog(null, "Oh, I see. I know where he is, do you want to know?", "", 0, 0, null);
                    if (help == 0) 
                    {
                        JOptionPane.showMessageDialog(null, "Okay, well get onto the spaceship and I'll bring you to him", "", 0, null);
                        JOptionPane.showMessageDialog(null, "You get onto to the spaceship and seconds later arrive to Planet 51", "", 0, null);
                        JOptionPane.showMessageDialog(null, "Alright dude, here you are. Good Luck", "", 0, null);
                        String[] options2 = {"I've come to retrieve the gaulet"};
                        JOptionPane.showOptionDialog(null, "...what do you want", "Thanos encounter", 0, 0, null, options2, null);
                        JOptionPane.showMessageDialog(null, "no can do, you need to defeat me first", "Thanos encounter", 0, null);
                        JOptionPane.showMessageDialog(null, "In order to beat Thanos, you must answer each question correctly and each correct answer is an attack", "Game Rules", 0, null);
                        String[] option3 = {"soul, mentality, power, reality, time, space", "soul, matter, power, mind, time, space", "soul, power, mind, time, space, reality"};
                        int questionOne = JOptionPane.showOptionDialog(null, "What are the six infinity stones?", "Question one", 0, 0, null, option3, null);
                        if (questionOne != 2) 
                        {
                            wrongQuestion();
                        } 
                        else 
                        {
                            String[] option4 = {"Satan", "Titan", "I dunno"};
                            int questionTwo = JOptionPane.showOptionDialog(null, "Where was Thanos born?", "Question Two", 0, 0, null, option4, null);
                            if (questionTwo != 1) 
                            {
                                wrongQuestion();

                            } 
                            else if (questionTwo == 1) 
                            {
                                String[] option5 = {"Odin", "Saturn", "Eternal A'lars"};
                                int questionThree = JOptionPane.showOptionDialog(null, "Thanos is the son of...", "Question Three", 0, 0, null, option5, null);
                                if (questionThree != 2) 
                                {
                                    wrongQuestion();
                                } 
                                else 
                                {
                                    JOptionPane.showMessageDialog(null, "Oh no...", "Success", 0, null);
                                    JOptionPane.showMessageDialog(null, "Mission is successful :)", "End Game", 0, null);
                                }
                            }
                        }
                    } 
                    else 
                    {
                        JOptionPane.showMessageDialog(null, "Oh, okay well I need to go and sadly must kick you out. Have a safe trip home. Bye", "Bye", 0, null);
                        JOptionPane.showMessageDialog(null, "Mission failed :(", "End Game", 0, null);
                    }
                }
            }
            else
            {
                
            }
        }
    
    }
    
    public static void wrongQuestion()
    {
        JOptionPane.showMessageDialog(null, "Ha, Good bye", "Defeat", 0, null);
        JOptionPane.showMessageDialog(null, "Mission failed :(", "End Game", 0, null);
    }
    
    
